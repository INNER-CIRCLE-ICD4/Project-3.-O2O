{
  "request": {
    "method": "GET",
    "urlPathPattern": "/api/v1/drivers/available",
    "queryParameters": {
      "h3Index": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "drivers": [
        {
          "id": "{{randomValue type='UUID'}}",
          "location": {
            "latitude": "{{randomValue type='DOUBLE' range='[37.49, 37.52]'}}",
            "longitude": "{{randomValue type='DOUBLE' range='[127.02, 127.07]'}}",
            "h3Index": "{{request.query.h3Index}}",
            "lastUpdated": "{{now}}"
          },
          "status": "AVAILABLE",
          "rating": "{{randomValue type='DOUBLE' range='[4.0, 5.0]'}}",
          "acceptanceRate": "{{randomValue type='DOUBLE' range='[0.8, 0.99]'}}",
          "completionRate": "{{randomValue type='DOUBLE' range='[0.9, 0.99]'}}",
          "vehicleType": "{{randomValue type='ENUM' values='STANDARD,PREMIUM,DELUXE'}}"
        },
        {
          "id": "{{randomValue type='UUID'}}",
          "location": {
            "latitude": "{{randomValue type='DOUBLE' range='[37.49, 37.52]'}}",
            "longitude": "{{randomValue type='DOUBLE' range='[127.02, 127.07]'}}",
            "h3Index": "{{request.query.h3Index}}",
            "lastUpdated": "{{now}}"
          },
          "status": "AVAILABLE",
          "rating": "{{randomValue type='DOUBLE' range='[4.0, 5.0]'}}",
          "acceptanceRate": "{{randomValue type='DOUBLE' range='[0.8, 0.99]'}}",
          "completionRate": "{{randomValue type='DOUBLE' range='[0.9, 0.99]'}}",
          "vehicleType": "STANDARD"
        }
      ],
      "totalCount": "{{randomValue type='INT' range='[1, 20]'}}",
      "timestamp": "{{now}}"
    },
    "transformers": ["response-template"]
  }
}